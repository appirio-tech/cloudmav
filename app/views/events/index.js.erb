$("<%= escape_javascript(render :partial => "events/event", :collection => @profile_events, :as => :event) %>").insertBefore(".event_load");
$("#events>li[data-type=event]").slideDown('slow');

$(".event_load").slideUp('slow');
$(".more_events").slideDown();

$("#filter").val("<%= @filter %>");
$("#subfilter").val("<%= @subfilter %>");
$("#page").val("<%= @page.to_i + 1 %>");

<% if @profile_events.count < 5 %>
  $("#more_events").val("No more to load");
  $("#more_events").attr("disabled", "true");
<% else %>
  $("#more_events").val("Click to load more");
  $("#more_events").removeAttr("disabled");
<% end %>
