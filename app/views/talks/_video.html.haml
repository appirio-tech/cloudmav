%li
  - if @talk.video?
    %a{:rel => "#video"}
      = image_tag "talk/movie.png"
    #video.overlay
      %video{:src => video_html(@talk.video_url), :width => "100px", :height => "100px;", :controls => "controls", :preload => "preload"}
      //= video_html @talk.video_url
  - else
    = image_tag "talk/movie_off.png"
